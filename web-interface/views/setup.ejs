  <!DOCTYPE html>
  <html>
    <head>
      <!--Import Google Icon Font-->
      <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link href="/page.css" rel="stylesheet">
      <!--Import materialize.css-->
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">

      <!--Let browser know website is optimized for mobile-->
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>
    <body class="teal lighten-1">
      <div class="row margin-top">
        <form class="col s4 card-panel offset-s4" method="post">
          <input type="hidden" value="<%= id%>" name="id">
          <h3 class="center-align">Setup Device</h3>
          <div class="row">
            <div class="input-field col s12">
              <input id="description" type="text" name="description" class="validate">
              <label for="description" data-error="Can't you fiqure out how to type an email?" data-success="">Description</label>
            </div>
            <button class="btn waves-effect waves-light" type="submit">Done
            <i class="material-icons right">done</i>
            </button>
            <button class="btn waves-effect waves-light" id="logout" type="reset">Restart
            <i class="material-icons right">replay</i>
            </button>
          </div>
        </form>
      </div>
      <!--Import jQuery before materialize.js-->
      <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"></script>
      <script>
      $("#logout").click(function(){
          window.location.href = "/logout";
      });
      </script>
    </body>
  </html>